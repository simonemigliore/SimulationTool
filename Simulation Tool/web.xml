<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginCheck</servlet-name>
		<servlet-class>com.smr.sessionutility.CheckLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginCheck</servlet-name>
		<url-pattern>/pages/checklogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CheckGoogleLogin</servlet-name>
		<servlet-class>com.smr.sessionutility.CheckGoogleLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckGoogleLogin</servlet-name>
		<url-pattern>/CheckGoogleLogin</url-pattern>
	</servlet-mapping>	
	<servlet>
		<servlet-name>getLogInformation</servlet-name>
		<servlet-class>com.smr.logger.getLogInformation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getLogInformation</servlet-name>
		<url-pattern>/getLogInformation</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>enqueue</servlet-name>
		<servlet-class>com.smr.dataset.Enqueue</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>enqueue</servlet-name>
		<url-pattern>/enqueue</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>worker</servlet-name>
		<servlet-class>com.smr.dataset.Worker</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>worker</servlet-name>
		<url-pattern>/worker</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>showData</servlet-name>
		<servlet-class>com.smr.simulation.query.RetrieveCurrentDataSimulation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showData</servlet-name>
		<url-pattern>/showData</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.smr.sessionutility.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login_access</servlet-name>
		<servlet-class>com.smr.sessionutility.CheckGoogleLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login_access</servlet-name>
		<url-pattern>/login_access</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>pages/login.html</welcome-file>
	</welcome-file-list>
</web-app>
